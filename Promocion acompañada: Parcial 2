def ejercicio1():
    t = [22, 24, 25, 27, 30, 29, 21, 20, 19, 23, 26, 28, 24, 25, 27, 31, 33, 32, 29, 28, 26, 24, 22, 21, 23, 25, 26, 30,
         31, 29]
    print("Promedio:", sum(t) / len(t))
    print("Días >25°:", sum(1 for x in t if x > 25))
    print("Más caluroso:", max(t))
    print("Más frío:", min(t))
ejercicio1()
